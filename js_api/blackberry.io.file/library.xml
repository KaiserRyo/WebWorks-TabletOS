<?xml version="1.0" encoding="utf-8"?>
<library>
	<extension id="blackberry.io.file">
		<entryClass>blackberry.io.file.FileExtension</entryClass>
	</extension>

	<platforms>
		<platform value="AIR">
			<target version="default" config="AIR_XHR" />
		</platform>
	</platforms>

	<configurations>
		<configuration name="AIR_XHR" version="1.0" comment="For XHR architecture for Java implementation">			
			<src type="text/javascript" path="js/common/file_dispatcher.js" comment="File JS XHR dispatcher" />
			<src type="text/javascript" path="js/common/file_ns.js" comment="File JS interface" />			
			<src type="text/actionscript" path="src/Air/File/src" comment="ActionScript implementation" />
		</configuration>
	</configurations>
	
	<features>
		<feature id="blackberry.io.file" version="1.0.0" />
	</features>	
</library>
